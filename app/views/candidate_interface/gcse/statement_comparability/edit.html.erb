<% content_for :title, title_with_error_prefix(grade_step_title(@subject, @qualification_type), @enic_form.errors.any?) %>
<% content_for :before_content, govuk_back_link_to(@previous_path) %>

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <%= form_with model: @enic_form, url: send("candidate_interface_edit_gcse_#{@subject}_statement_comparability_path", subject: @subject), method: :patch do |f| %>
      <%= render 'form', f: f %>
    <% end %>
  </div>
</div>
