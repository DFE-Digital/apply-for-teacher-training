<table class="govuk-table">
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th scope="col" class="govuk-table__header">Match</th>
      <th scope="col" class="govuk-table__header">Date</th>
    </tr>
  </thead>

  <tbody class="govuk-table__body">
    <% @matches.each do |match| %>
      <tr class="govuk-table__row">
        <td class="govuk-table__cell">
          <%= govuk_link_to SupportInterface::DuplicateMatchesTableComponent.description_for(match), support_interface_duplicate_match_path(match) %>
        </td>
        <td class="govuk-table__cell">
          <%= match.created_at.to_s(:govuk_date_short_month) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
